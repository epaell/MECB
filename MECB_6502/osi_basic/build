ca65 -D ACIA='$C008' --cpu 6502 -l osi_bas_16KB.lst -o osi_bas_16KB.o src/osi_bas.asm  
ca65 -D ACIA='$C008' --cpu 6502 -l osi_bas_32KB.lst -o osi_bas_32KB.o src/osi_bas.asm  
ca65 -D ACIA='$C008' --cpu 6502 -l osi_bas_8KB.lst -o osi_bas_8KB.o src/osi_bas.asm  
ld65 -C cfg/ld65_mecb.cfg -o AT28C256_basic_8KB.bin osi_bas_8KB.o
ld65 -C cfg/ld65_mecb.cfg -o AT28C256_basic_16KB.bin osi_bas_16KB.o
ld65 -C cfg/ld65_mecb.cfg -o AT28C256_basic_32KB.bin osi_bas_32KB.o

